{
  "name": "tonapi-sdk-js",
  "version": "0.40.0",
  "description": "Autogenerated SDK by open-api for tonapi.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "rm:old": "rm -rf src/apis && rm -rf src/models && rm -rf dist",
    "generate:sdk": "yarn rm:old && docker build . -t tonapisdk && docker run --rm -v $PWD:/local tonapisdk && tsc"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/startfellows/tonapi-sdk-js/issues"
  },
  "homepage": "https://github.com/startfellows/tonapi-sdk-js#readme",
  "repository": "https://github.com/startfellows/tonapi-sdk-js",
  "keywords": [
    "TON",
    "Blockchain",
    "API"
  ],
  "author": {
    "name": "mdusafp"
  },
  "devDependencies": {
    "git-cz": "^4.9.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "react-arborist": "^1.2.0"
  }
}
